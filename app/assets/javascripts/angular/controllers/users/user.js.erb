app.controller('UserCtrl', ['$scope', '$http', '$auth', '$location','$routeParams', function($scope, $http, $auth, $location,$routeParams){

  var url = "<%= ENV['URL'] %>" || "http://localhost:3000";
  
  $http.get(url + '/users/' + $routeParams.id).success(
    function(response){
      $scope.user = response
    })

}]);